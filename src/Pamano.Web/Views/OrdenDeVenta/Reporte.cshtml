@{
    Layout = null;

}

<!DOCTYPE HTML>
<html>
<head>
    <link rel="stylesheet" type="text/css" >
    

</head>
<body > 
	<script>
		window.onload = function () {
		
		var chart = new CanvasJS.Chart("chartContainer", {
			animationEnabled: true,
			theme: "light2", // "light1", "light2", "dark1", "dark2"
			title: {
				text: "Reporte de ventas - Mes X"
			},
			axisY: {
				title: "Total vendido por producto",
				
			},
			axisX: {
				title: "Producto"
			},
			data: [{
				type: "column",
				dataPoints: [
                    {y: 40.000 , label: "Borradores" },
                    {y: 100.000 , label: "Cartulina blanca" },
					{ y: 200.000, label: "Lapices" },	
                    { y: 400.000 , label: "Esferos" },
                    {y: 500.000 , label: "Cuadernos norma" }
					
				]
			}]
		});
		chart.render();
		
		}
		</script>
    
<div  id="chartContainer" style="height:500px; width:auto; padding-top: 20px; background-image: url(../img/fondo3.png);"  ></div>



<script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
</body>
</html>