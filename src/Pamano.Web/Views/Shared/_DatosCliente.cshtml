<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Datos Personales</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/css_cliente/estilos_datosp_clien.css">
    <link rel="shortcut icon" href="~/img/Logo1.png">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
</head>
<body>
        <nav>
            <div class="logo">
                <img src="../img/Logo1.png" alt="">
            </div>

            <div class="navegacion">
                <input type="text" name="Buscador" placeholder="Buscar" class="Buscador">
                <p class="padding"><a href="#" class="btn_buscar">Buscar</a></p>

                <div class="list_navegacion">
                    <ul>
                        <li><a href="#">INICIO</a></li>
                        <li>
                            <a href="#">MARCAS</a>
                            <ul>
                                <li><a href="#">Escolares</a></li>
                                <li><a href="#">Oficina</a></li>
                                <li><a href="#">Detalles</a></li>
                            </ul>
                        </li>
                        <li><a href="#">QUIENES SOMOS</a></li>
                        <li>
                            <a href="#">CONTACTENOS</a>
                            <ul>
                                <li class="Direccion"><a href="#">Direccion</a></li>
                                <li><a href="#">Redes Sociales</a></li>
                                <li><a href="#">Correo Electronico</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>

            <div class="cerrar">
                <a href="#">Cerrar sesion</a>
            </div>
        </nav>
    <header class="fondo_tifblue">
        <h1 class="fondo_tifblue indice">Inicio - Mi Cuenta - Datos Personales</h1>
    </header>
    <div>
        <main role="main" class="pb-3">
            <div class="Formulario">

                <form class="form-registro" onsubmit="return" action="#" method="post">

                    <h2 class="form-titulo">Datos Personales</h2>

                    <div class="cont-input">

                        <input type="text" name="Nombres" placeholder="Nombres" class="input-50">

                        <input type="text" name="Apellidos" placeholder="Apellidos" class="input-50">

                        <input type="email" name="Correo" placeholder="Correo electronico" class="input-100">

                        <input type="text" name="Usuario" placeholder="Cedula" class="input-50">

                        <input type="text" name="Telefono" placeholder="Telefono" class="input-50">

                        <input type="button" onclick="location.href='../html/DatosP_Cliente_Contra.html'" value="Cambiar Contraseña" class="btn-regis btn-enviar">

                        <input type="submit" value="Guardar" class="btn-regis btn-enviar">

                        <p class="form-link"><a class="form-link" href="../html/mi_cuenta_cliente.html">Volver</a></p>

                    </div>
                </form>

            </div>
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - Pamano.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>