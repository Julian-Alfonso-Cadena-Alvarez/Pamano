<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Pamano</title>
    <link rel="stylesheet" type="text/css" href="~/css_cliente/estilos.css">
    <link rel="shortcut icon" href="../img/Logo1.png">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
</head>
<body>
    <nav>
        <div class="logo">
            <img src="../img/Logo1.png" alt="">
        </div>

        <div class="navegacion">
            <input type="text" name="Buscador" placeholder="Buscar" class="Buscador">
            <p class="padding"><a href="#" class="btn_buscar">Buscar</a></p>

            <div class="list_navegacion">
                <ul>
                    <li><a href="#">INICIO</a></li>
                    <li>
                        <a href="#">MARCAS</a>
                        <ul>
                            <li><a href="#">Escolares</a></li>
                            <li><a href="#">Oficina</a></li>
                            <li><a href="#">Detalles</a></li>
                        </ul>
                    </li>
                    <li><a href="#QuienesSomos">QUIENES SOMOS</a></li>
                    <li>
                        <a href="#">CONTACTENOS</a>
                        <ul>
                            <li ><a href="#">Direccion</a></li>
                            <li><a href="#">Redes Sociales</a></li>
                            <li><a href="#">Correo Electronico</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>

        <div class="cerrar">
            <a href="#">Iniciar Sesion</a>
            <a href="#">Resgistrarse</a>
        </div>
    </nav>

    <!-- Encabezado -->
    <header class="fondo_tifblue">

        <div class="Lineas ">
            <ul>
                <li class="fondo_tifblue ">
                    <a href="#" class="indice">Inicio</a>
                    <a href="#" class="indice">Linea Escritura</a>
                    <a href="#" class="indice">Linea Escolar</a>
                    <a href="#" class="indice">Linea Oficina</a>
                    <a href="#" class="indice">Linea Archivo</a>
                    <a href="#" class="indice">Linea Detalles</a>
                    <!-- <a href="#" class="indice">Mi Perfil</a>-->
                    <a class="indice indice fav-car" asp-area="" asp-controller="Home" asp-action="PerfilCLiente">Mi Perfil</a>
                </li>
            </ul>
        </div>
    </header>
    <div>
        <main role="main" class="pb-3">
            <div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="../img/MarcadoresExpo.jpeg" alt="Primer slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 h-90" src="../img/Resaltadores.jpeg" alt="segundo slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100  " src="../img/Colores.jpeg" alt="tercero slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 " src="../img/Esferos.jpeg" alt="cuarto slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 " src="../img/CuadernosScribe.jpeg" alt="Quinto slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 " src="../img/CuadernosKiut.jpeg" alt="Sexto slide">
                    </div>
                </div>
            </div>
            <div class="div-NuestrasMarcas">
                <div class="div-Titulo">
                    <h2 class="Titulo">Nuestras Marcas</h2>
                </div>
                <div class="div-marcas">
                    <div class="borde cont-marca">

                    </div>
                    <div class="borde cont-marca">

                    </div>
                    <div class="borde cont-marca">

                    </div>
                    <div class="borde cont-marca">

                    </div>
                    <div class="borde cont-marca">

                    </div>
                    <div class="borde cont-marca">

                    </div>
                </div>
            </div>
            <div class=" div-QuienesSomos">
                <div class="div-Titulo">
                    <h2 id="#QuienesSomos" class="Titulo">Quienes Somos</h2>
                </div>
                <div class="div-Info">
                    <div class="iconoVision">
                        <img src="../img/vision.png" alt="">
                    </div>
                    <div class="textoQuienes">
                        <h3>VISION</h3>
                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam</p>
                    </div>
                </div>
                <div class="div-Info">
                    <div class="iconoVision">
                        <img src="../img/mision.png" alt="">
                    </div>
                    <div class="textoQuienes">
                        <h3>MISION</h3>
                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam</p>
                    </div>
                </div>
                <div class="div-Info ">
                    <div class="iconoprov">
                        <img src="../img/inventario.png" alt="">
                    </div>
                    <div class="textoQuienes">
                        <h3>PROVEEDORES</h3>
                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam</p>
                    </div>
                </div>
            </div>
            <div class="CONTACTENOS">
                <div class="div-Titulo">
                    <h2 class="Titulo">Contactenos</h2>
                </div>
                <div class="form-contacto borde">
                    <form class="form-registro" onsubmit="return" action="#" method="post">

                        <h2 class="form-titulo">Contacta con nosotros</h2>
                        <div class="cont-input">
                            <input type="text" name="Nombres" placeholder="Nombres" class="input-50">
                            <input type="text" name="Apellidos" placeholder="Apellidos" class="input-50">
                            <input type="email" name="Correo" placeholder="Correo electronico" class="input-100">
                            <input type="text" name="Mensaje" placeholder="Cuentanos" class="input-100 text-enviar">
                            <input type="submit" value="Enviar" class="btn-enviar">
                        </div>
                    </form>

                </div>
                <div class="mapa borde">

                </div>
                <div class="borde Direccion">

                </div>

            </div>
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - Pamano <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
